width: 72rpx;
border-bottom: 8rpx solid #ADD651;
}
.body .oneinfo{
	max-width: 90vw;
	margin: 26rpx auto;
	background: #FFFFFF;
  box-shadow: 0px 2rpx 16rpx rgba(0, 0, 0, 0.25);
	border-radius: 20rpx;
	padding: 30rpx;
	box-sizing: border-box;
}
.oneinfo .title{
	width: 100%;
	display: flex;
	align-items: center;
	justify-content: space-between;
	margin-bottom: 26rpx;
}
.oneinfo .title .tou{
	width: 80rpx;
	height: 80rpx;
	border-radius: 40rpx;
	margin-right: 18rpx;
}
.oneinfo .title .tou image{
	width: 100%;
	height: 100%;
	border-radius: 40rpx;
}
.oneinfo .title .person .boxone{
		display: flex;
		align-items: center;
		justify-content: center;
}
.boxone .nickname{
	font-family: 'Source Han Sans CN';
	font-style: normal;
  font-weight: 600;
  font-size: 32rpx;
  line-height: 36rpx;
	color: #000000;
	margin-right: 10rpx;
}
.boxone .sex image{
	width: 24rpx;
	height: 24rpx;
}
.boxtwo {
	font-family: 'Source Han Sans CN';
	font-style: normal;
  font-weight: 400;
  font-size: 24rpx;
  line-height: 30rpx;
	color: rgba(0, 0, 0, 0.4);
	display: flex;
	align-items: center;
}
.boxtwo .txtone{
	margin-right: 8rpx;
}
.oneinfo .xianzhi{
	width: 100%;
	box-sizing: border-box;
}
.oneinfo .content{
	position: relative;
	width: 100%;
	font-family: 'Source Han Sans CN';
  font-style: normal;
  font-weight: 400;
  font-size: 28rpx;
  line-height: 40rpx;
	color: #000000;
}
.yincang{
	overflow: hidden;
	text-overflow: ellipsis;
	display: -webkit-box;
	-webkit-box-orient: vertical;
	-webkit-line-clamp: 6;
}
.zhanshi{
		position: absolute;
		right: 0;
		bottom: 0;
		background-color: #fff;
		z-index: 999;
		color: #ADD651;
		width: 130rpx;
		font-size: 30rpx;
		display: flex;
		flex-wrap: nowrap;
		align-items: center;
		justify-content: center;
}
.oneinfo .imglist{
	width: 100%;
	margin-top: 26rpx;
	display: flex;
}
.oneinfo .imglist .oneimg{
	width: 524rpx;
	height: 524rpx;
}
.oneinfo .imglist .oneimg image{
	width: 100%;
	height: 100%;
	border-radius: 20rpx;
}
.oneinfo .imglist .twoimg{
	width: 268rpx;
	height: 268rpx;
	margin-right: 6rpx;
}
.oneinfo .imglist .twoimg image{
	width: 100%;
	height: 100%;
	border-radius: 20rpx;
}
.oneinfo .imglist .twoimg:nth-child(1) image{
			border-top-right-radius: 0;
			border-bottom-right-radius: 0;
}
.oneinfo .imglist .twoimg:nth-child(2) image{
  border-top-left-radius: 0;
	border-bottom-left-radius: 0;
}
.package{
	width: 100%;

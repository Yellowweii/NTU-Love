.body{
	width: 100vw;
}
swiper{
	width: 100vw;
	height: 45vh;
}
swiper-item{
	width: 100%;
	height: 100%;
}
swiper-item image{
	width: 100%;
	height: 100%;
	border-top-left-radius: 20rpx;
	border-top-right-radius: 20rpx;
}
.body .info{
	width: 90vw;
	margin: 0 auto;
	height: 300rpx;
	background: #FFFFFF;
  box-shadow: 0px 2rpx 16rpx rgba(0, 0, 0, 0.25);
	border-radius: 20rpx;
	transform: translateY(-50rpx);
	display: flex;
	flex-direction: column;
	justify-content: space-around;
	padding: 20rpx 34rpx;
	box-sizing: border-box;
	font-family: 'Inter';
font-style: normal;
font-weight: 400;
font-size: 28rpx;
line-height: 30rpx;
color: rgba(0, 0, 0, 0.4);
}
.info .left{
	margin-right: 14rpx;
	font-family: 'Inter';
  font-style: normal;
  font-weight: 600;
  font-size: 50rpx;
  line-height: 58rpx;
  color: #000000;
}
.info .icon image{
	width: 40rpx;
	height: 40rpx;
	margin-right: 14rpx;
}
.info .right image{
	width: 40rpx;
	height: 40rpx;
}
.bao{

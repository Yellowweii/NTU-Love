.confirm{
	width: 100%;
	background: #d9d9d975;
}
.confirm .nav{
	width: 100vw;
	background-color: #fff;
	display: flex;
	align-items: center;
	justify-content: center;
	padding-bottom: 10rpx;
}
.confirm .nav .box{
	width: 240rpx;
	height: 100%;
	display: flex;
	align-items: center;
	justify-content: center;
	font-family: 'Source Han Sans CN';
font-style: normal;
font-weight: 700;
font-size: 40rpx;
line-height: 72rpx;
color: rgba(0, 0, 0, 0.4);
}
.confirm .nav .box .one,.two{
	box-sizing: border-box;
}
.active{
		font-size: 48rpx;
		color: #000;
		border-bottom: 8rpx solid #ADD651;
}
.confirm .info{
	max-width: 100vw;
	padding: 30rpx 50rpx;
	background-color: #fff;
	margin-bottom: 12rpx;
}
.info .title{
	display: flex;
	align-items: center;
	justify-content: space-between;
	margin-bottom: 26rpx;
}
.info .title .tou{
	width: 80rpx;
	height: 80rpx;
	border-radius: 40rpx;
	margin-right: 18rpx;
}
.info .title .tou image{
	width: 100%;
	height: 100%;
	border-radius: 40rpx;
}
.info .title .person .boxone{
		display: flex;
		align-items: center;
		margin-bottom: 5rpx;
}
.boxone .nickname{
	font-family: 'Source Han Sans CN';
	font-style: normal;
  font-weight: 600;
  font-size: 32rpx;
  line-height: 36rpx;
	color: #000000;
	margin-right: 10rpx;
}
.boxone .sex image{
	width: 24rpx;
	height: 24rpx;
}
.boxtwo {
	font-family: 'Source Han Sans CN';
	font-style: normal;
  font-weight: 400;
  font-size: 24rpx;
  line-height: 30rpx;
	color: rgba(0, 0, 0, 0.4);
	display: flex;
	align-items: center;
}
.boxtwo .txtone{
	margin-right: 8rpx;
}
.info .xianzhi{
	width: 100%;
	box-sizing: border-box;
}
.info .content{
	position: relative;
	width: 100%;
	font-family: 'Source Han Sans CN';
  font-style: normal;
  font-weight: 400;
  font-size: 28rpx;
  line-height: 40rpx;
	color: #000000;
}
.yincang{
	overflow: hidden;
	text-overflow: ellipsis;
	display: -webkit-box;
	-webkit-box-orient: vertical;
	-webkit-line-clamp: 6;
}
.zhanshi{
		position: absolute;
		right: 0;
		bottom: 0;
		background-color: #fff;
		z-index: 100;
		color: #ADD651;
		width: 130rpx;
		font-size: 30rpx;
		display: flex;
		flex-wrap: nowrap;
		align-items: center;
		justify-content: center;
}
.info .imglist{
	width: 100%;
	margin-top: 26rpx;
	display: flex;
}
.info .imglist .oneimg{
	width: 524rpx;
	height: 524rpx;
}
.info .imglist .oneimg image{
	width: 100%;
	height: 100%;
	border-radius: 20rpx;
}
.info .imglist .twoimg{
	width: 268rpx;
	height: 268rpx;
	margin-right: 6rpx;
}
.info .imglist .twoimg image{
	width: 100%;
	height: 100%;
	border-radius: 20rpx;
}
.info .imglist .twoimg:nth-child(1) image{
			border-top-right-radius: 0;
			border-bottom-right-radius: 0;
}
.info .imglist .twoimg:nth-child(2) image{
  border-top-left-radius: 0;
	border-bottom-left-radius: 0;
}
